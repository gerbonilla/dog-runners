<h1>Runs#index</h1>
<p>Find me in app/views/runs/index.html.erb</p>

<div class="container">
  <div class="row">
    <div class="col col-xs-12">

      <% @runs.each do |run| %>
      <div class="dog-runners-box">
      <p>Status: <%= run.status  %></p>
      <p>Appointment: <%= run.appointment  %></p>
      <p>Duration: <%= run.duration  %></p>
      <p>Price: <%= run.price  %></p>
      <p>Note: <%= run.note  %></p>
      </div>
      <% end %>

    </div>
  </div>
</div>



