<h1>See all runs</h1>


<div class="container">
  <div class="row">
    <div class="col col-xs-12">
      <% @runs.each do |run| %>
      <div class="dog-runner-box">
      <p>Status: <%= run.status  %></p>
      <p>Appointment: <%= run.appointment  %></p>
      <p>Duration: <%= run.duration  %></p>
      <p>Price: <%= run.price  %></p>
      <p>Note: <%= run.note  %></p>
      </div>
      <% end %>

    </div>
  </div>
</div>



